/*每张图片所占容器*/
.itemImage {
    position: relative;
    margin: 2rpx;
    padding: 0.5rpx;
    background-color: #7a7a7a;
}


/*小图片*/
.thumb {
	margin: 10rpx;
  height: 200rpx;
  width: 200rpx;
  position: relative;
}


/*删除按钮*/
.delete {
	position: absolute;
	height: 40rpx;
	top: 0rpx;
  right:0;
	width: 40rpx;
}

.delete image {
	position: absolute;
	width: 30rpx;
	height: 30rpx;
}

