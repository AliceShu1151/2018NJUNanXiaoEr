.titleAndDetail{
   background-color: #ffffff;
   margin-top: 10rpx;
   position: relative;
   overflow:hidden;


}
.titleAndDetail::before {
content:" ";
position:absolute;
left:0;
top:0;
right:0;
height:1px;
border-top:1rpx solid #E5E5E5;
color:#E5E5E5;

}
.titleAndDetail::after {
content:" ";
position:absolute;
left:0;
bottom:0;
right:0;
height:1px;
border-bottom:1rpx solid #E5E5E5;
color:#E5E5E5;

}
