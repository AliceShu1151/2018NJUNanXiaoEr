
.notice {
  width: 750rpx;
  padding: 25rpx;
}
.title {
  color:black;
  font-size: 40rpx;
  margin-bottom: 30rpx;
}
.time {
  color:#D9D9D9;
  font-size: 30rpx;
  margin-bottom: 30rpx;
}
.time_after {
  content: " ";
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1rpx solid #D9D9D9;
  color: #D9D9D9;
}
.text {
  margin-left: 20rpx;
  margin-right: 50rpx;
  font-size: 35rpx;
}
.content-list {
  display:flex;
  flex-direction:column;
  padding-right: 30rpx;
  padding-top: 20rpx;
}